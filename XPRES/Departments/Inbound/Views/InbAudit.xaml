<Window x:Class="XPRES.Departments.Inbound.Views.InbAudit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:XPRES.Departments.Inbound.Views"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="InbAudit"
        Width="1280"
        Height="720"
        mc:Ignorable="d">
    <Grid Background="{StaticResource SolidBackDark}">
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Border Margin="5" Style="{StaticResource CustomBorder}" />
        <Border Grid.Row="1"
                Margin="5"
                Style="{StaticResource CustomBorder}">
            <Grid>
                <DataGrid ItemsSource="{Binding}" Style="{StaticResource CustomDataGrid}" />
            </Grid>
        </Border>
    </Grid>
</Window>
